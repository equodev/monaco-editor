<?xml version="1.0" encoding="UTF-8"?>
<plugin id="com.equo.eclipse.monaco.editor">
	<requires>
		<import plugin="org.eclipse.core.runtime" />
		<import plugin="org.eclipse.swt" />
	</requires>
	<extension point="org.eclipse.ui.editors">
		<editor
			class="com.equo.eclipse.monaco.editor.MonacoEditorPart"
			default="true"
			id="com.equo.eclipse.monaco.editor.EquoEditor"
			icon="img/equo.png"
			name="Equo Editor">
		    <contentTypeBinding
               contentTypeId="org.eclipse.core.runtime.text">
            </contentTypeBinding>
		</editor>
	</extension>
</plugin>
