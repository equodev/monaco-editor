-include: https://dl.dev.equoplatform.com/framework/1.0.0/framework.bnd

-plugin.1.Equo: \
    aQute.bnd.repository.osgi.OSGiRepository; \
        name = Equo; \
        locations = "https://dl.dev.equoplatform.com/framework/${framework_version}/repo/index.xml.gz"; \
        poll.time = -1; \
        cache = ~/.bnd/cache/equo

-plugin.2.EquoExtras: \
    aQute.bnd.repository.osgi.OSGiRepository; \
        name = EquoExtras; \
        locations = "https://dl.dev.equoplatform.com/framework-extras/${framework_extras_version}/repo/index.xml.gz"; \
        poll.time = -1

